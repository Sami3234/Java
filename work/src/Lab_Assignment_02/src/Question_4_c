import java.util.Scanner;

public class Question_4_c {
    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
        int x;

        System.out.print("Enter any integer: ");
        x = in.nextInt();

        if (x > 0)
            System.out.println("1");
        else if (x < 0)
            System.out.println("-1");
        else
            System.out.println("0");

        in.close();
    }
}
